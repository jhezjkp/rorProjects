<h1>Editing post</h1>

<%= simple_form_for @post, :url => board_post_path(@board, @post), 
  :html => { :multipart => true } do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
<% end %>

<%= link_to 'Show', board_post_path(@board, @post) %> |
<%= link_to 'Back', board_post_path(@board) %>
